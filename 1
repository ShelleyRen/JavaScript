    <title>Title</title>
    <script>
      function sum (arr, tar){
        let obj = {};
        for(let i=0; i<arr.length; i++){
          let item = arr[i];
          if(obj[item] === undefined){
            let x = tar - item;
            obj[x] = i;
          }
          else{
            return [obj[item], i];
          }
        }
        return null;

      }
       console.log(sum([2,11,7,15], 9))
    </script>
  </head>
  <body>
  </body>
